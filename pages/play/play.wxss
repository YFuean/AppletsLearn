page{
  background: #17181a;
}
.font-white{
  color: #a8a9a9;
}
.font-30{
  font-size: 30rpx;
}
.font-24{
  font-size: 24rpx;
}
.content{
  margin: auto;
  width: 96%;
  background:#232623;
}
.width-100{
  width: 100%;
}
.top{
  height: 150rpx;
  margin-top: 50rpx;
  margin-bottom: 50rpx;
  text-align: center;
}
.play_img{
  margin: auto;
  width: 400rpx;
  height: 400rpx;
  margin-top: 100rpx;
  margin-bottom: 100rpx;
}
.play_image{
  width: 100%;
  height: 100%;
  border-radius: 50%;
}
.slider_box{
  margin: auto;
  width: 80%;
  height: 80rpx;
  margin-top: 200rpx;
}
.slider_bar{
  size: 30rpx;
}
.play_list{
  margin: auto;
  margin-top: 30rpx;
  width: 90%;
  height: 100rpx;
  display: flex;
  flex-wrap: wrap;
  background:#232623;
}
.avatar{
  width: 100rpx;
  height: 100rpx;
}
.mu_avatar{
  width: 100rpx;
  height: 100rpx;
  border-radius: 8rpx;
}
.ml{
  margin-left: 20rpx;
  width: 45%;
}
.icon_box{
  display: flex;
  flex-wrap: wrap;
  margin-left: 50rpx;
}
.icon_item{
  margin-right: 20rpx; 
  display: flex;  
  align-items: center;
  justify-content: center;  
}

.icon-font-color{
  font-size:40rpx;
  color:#a8a9a9;

}
.iconfont {
  font-family: "iconfont" !important;
  font-style: normal;
  -webkit-font-smoothing: antialiased;
  -moz-osx-font-smoothing: grayscale;
}

.icon-bofang:before {
  content: "\e62f";
}

.icon-bofang1:before {
  content: "\e601";
}

.icon-liebiao:before {
  content: "\e604";
}

.icon-play-next:before {
  content: "\e624";
}

@font-face {
  font-family: 'iconfont';
  src: url(data:application/font-woff2;charset=utf-8;base64,) format('woff2'),
      url(data:application/font-woff;charset=utf-8;base64,) format('woff'),
      url('iconfont.ttf') format('truetype');
  font-weight: normal;
  font-style: normal;
}