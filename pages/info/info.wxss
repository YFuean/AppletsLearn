.row{
  display: flex;
  flex-wrap: wrap;
}
page{
  background: #17181a;
}
.col{
  margin: auto;
  text-align: center;
}
.col-li-30{
  /* 垂直居中3个 */
  margin-top: 30rpx;
}
.col-3{
  height: 175rpx;
  width: 175rpx;
  margin: 30rpx;
}
.col-2{
  height: 300rpx;
  width: 300rpx;
  margin: 30rpx;
}
.col-5{
  height: 115rpx;
  width: 115rpx;
  margin: 13rpx;
}
.contain{
  width: 96%;
  margin: auto;
}
.font-24{
  font-size: 24rpx;
}
.font-red{
  color: #d7242d;
}
.font-white{
  color: #a8a9a9;
}
.text-right{
  text-align: right;
  float: right;
}
.mr-3{
  margin-right: 1rem;
}
.img-view{
  width: 100%;
  height: 340rpx;
}
.pageTwo .swiperClass {margin: 0;margin-top: 10px;}
.pageTwo .slide-image {
  width: 100%;height: 90%;border-radius: 10px;position: relative;box-shadow: 0 0 10rpx rgba(0, 0, 0, .8)}
.pageTwo image.active {transform: none;transition: all 0.2s ease-in 0s;}
.pageTwo image.quiet {transform: scale(0.8333333);transition: all 0.2s ease-in 0s;}
.my_img{
  width: 100%;
  height: 230rpx;
}
.icon-font-color{
  font-size:70rpx;
  color:#a8a9a9;
}
.iconfont {
  font-family: "iconfont" !important;
  font-style: normal;
  -webkit-font-smoothing: antialiased;
  -moz-osx-font-smoothing: grayscale;
}

.icon-fm:before {
  content: "\e629";
}

.icon-tuijian:before {
  content: "\e614";
}

.icon-depuyouhuaxiangqingbangdanpaiming:before {
  content: "\e64b";
}


@font-face {
  font-family: 'iconfont';
  src: url(data:application/font-woff2;charset=utf-8;base64,) format('woff2'),
      url(data:application/font-woff;charset=utf-8;base64,) format('woff'),
      url('iconfont.ttf') format('truetype');
  font-weight: normal;
  font-style: normal;
}
