.row{
  display: flex;
  flex-wrap: wrap;
}
.col{
  margin: auto;
  text-align: center;
}
.col-li-30{
  /* 垂直居中3个 */
  margin-top: 30rpx;
}
.col-3{
  height: 175rpx;
  width: 175rpx;
  margin: 30rpx;
}
.col-2{
  height: 300rpx;
  width: 300rpx;
  margin: 30rpx;
}
.col-5{
  height: 115rpx;
  width: 115rpx;
  margin: 13rpx;
}
.contain{
  width: 96%;
  margin: auto;
}
.con{
  width: 50%;
}
.font-24{
  font-size: 24rpx;
}
.font-red{
  color: #d7242d;
}
.font-white{
  color: white;
}
.text-right{
  text-align: right;
  float: right;
}
.mr-3{
  margin-right: 1rem;
}
.img-view{
  width: 100%;
  height: 340rpx;
}
.pageTwo .swiperClass {margin: 0;margin-top: 10px;}
.pageTwo .slide-image {
  width: 100%;height: 90%;border-radius: 10px;position: relative;box-shadow: 0 0 10rpx rgba(0, 0, 0, .8)}
.pageTwo image.active {transform: none;transition: all 0.2s ease-in 0s;}
.pageTwo image.quiet {transform: scale(0.8333333);transition: all 0.2s ease-in 0s;}
.my_img{
  width: 100%;
  height: 230rpx;
}
.start{
  margin-top: 30rpx;
  width: 100%;
  height: 80rpx;
  background-color: #d7242d;
  border-radius: 50rpx;
  display: flex;  
  align-items: center;
  justify-content: center;
  box-shadow: 0 8rpx 16rpx 0 rgba(0, 0, 0, 2);
}

.iconfont {
  font-family: "iconfont" !important;
  font-size: 16px;
  font-style: normal;
  -webkit-font-smoothing: antialiased;
  -moz-osx-font-smoothing: grayscale;
}
.icon-font-color{
  font-size:56rpx;
  color:#d62f36;
}
.icon-huiyuan:before {
  content: "\e60d";
}

.icon-kafei:before {
  content: "\e62c";
}

.icon-wodedingdan:before {
  content: "\e635";
}

.icon-kfc:before {
  content: "\e62d";
}

.icon-wodeqiabao:before {
